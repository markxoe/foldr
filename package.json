{
  "name": "electron-windows-explorer-thing",
  "version": "0.0.1",
  "main": "index.js",
  "dependencies": {
    "electron-store": "^8.0.0"
  },
  "description": "Switches between Windows Explorer Tabs",
  "author": "markxoe",
  "scripts": {
    "start": "electron .",
    "build-win": "electron-builder --win --publish never",
    "publish-win": "electron-builder --win --publish onTagOrDraft"
  },
  "devDependencies": {
    "electron": "^12.0.5",
    "electron-builder": "^22.10.5"
  },
  "build": {
    "productName": "Explorer Switcher",
    "win": {
      "target": "nsis"
    },
    "nsis": {
      "oneClick": false
    }
  }
}
