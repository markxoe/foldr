{
  "name": "explorer-switcher",
  "version": "0.1.0",
  "main": "index.js",
  "dependencies": {
    "electron-store": "^8.0.0",
    "electron-updater": "^4.3.9"
  },
  "description": "Switches between Windows Explorer Tabs",
  "author": {
    "name": "Mark Oude Elberink",
    "email": "mark@oude-elberink.de"
  },
  "scripts": {
    "start": "electron .",
    "test": "node test/test.js",
    "build-win": "electron-builder --win --publish never",
    "publish-win": "electron-builder --win --publish onTagOrDraft",
    "build-mac": "electron-builder --mac --publish never",
    "publish-mac": "electron-builder --mac --publish onTagOrDraft",
    "build-all": "electron-builder -mw --publish never",
    "publish-all": "electron-builder -mw --publish onTagOrDraft"
  },
  "devDependencies": {
    "electron": "^12.0.7",
    "electron-builder": "^22.11.3",
    "spectron": "^14.0.0"
  }
}
